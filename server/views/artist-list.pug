extends main

block append css

block append content
    div(class="item-list-box")
        each item in locals.data.items
            div(class="item-box")
                div(class="img-box")
                    img(src="/client/img/flaticon/note.svg"
                        alt=item.artist_name width="50" height="50")
                div(class="content-box")
                    div(class="content-box-dsc"): a(href="")
                    div(class="content-box-name"): a(href="")=item.artist_name